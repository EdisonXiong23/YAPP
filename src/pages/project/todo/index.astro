---
import ProjectLayout from '../../../layouts/ProjectLayout.astro';

import { get_user_session } from '../../../firebase/server';
import Todo from '../../../components/Todo.astro'
const user = await get_user_session(Astro.cookies);
if(!user) {
  return Astro.redirect("/signin");
}
---

<ProjectLayout title="To-Do">
    <Todo user={user} />
</ProjectLayout>